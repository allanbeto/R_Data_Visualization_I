ltyExamples <- function(){  #
lineTypes <- 1:6
lineColors <- c("red", "green", "blue",
"black", "cyan", "purple")
plot(lineTypes, lineTypes, type = "n",
axes = FALSE, xlab = "", ylab = "")
segments(2, lineTypes, 6, lineTypes, lwd = 2,
lty = lineTypes, col = lineColors)
text(1, lineTypes, lineTypes, font = 2,
col = lineColors)
}
ltyExamples()
